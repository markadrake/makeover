{
	"name": "Makeover",
	"description": "Put lipstick on ugly pig websites.",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
		  "matches": ["*://*.twitter.com/*"], 
		  "js": ["twitter.min.js"],
		  "css" : ["twitter.min.css"]
		},
		{
			"matches": ["*://*.ign.com/*"], 
			"js": ["ign.min.js"],
			"css" : ["ign.min.css"]
		  }
	  ]	
}